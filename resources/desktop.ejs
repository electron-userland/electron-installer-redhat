[Desktop Entry]
Name=<%= productName %>
<% if (description) { %>Comment=<%= description %><% } %>
GenericName=<%= genericName %>
Exec=<%= name %> %U<% if (execArguments && execArguments.length) { %> <%= execArguments.join(' ') %><% } %>
Icon=<%= name %>
Type=Application
StartupNotify=true
Categories=<%= categories.join(';') %>;
<% if (mimeType.length) { %>MimeType=<%= mimeType.join(';') %>;<% } %>
